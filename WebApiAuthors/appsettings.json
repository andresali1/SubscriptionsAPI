{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "LimitRequests": {
    "FreeRequestsPerDay": 20,
    "RouteWhiteList": [
      "api/v1/account/register",
      "api/v1/account/login",
      "api/v1/account/RenewToken",
      "api/v1/account/BecomeAdmin",
      "api/v1/account/RemoveAdmin",
      "api/v1/account/BecomeAdmin",
      "api/apikey",
      "favicon.ico",
      "api/domainrestriction",
      "api/iprestriction"
    ]
  }
}
